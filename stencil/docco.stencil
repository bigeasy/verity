<!DOCTYPE html>
<html>
<head>
<title>={ @file }</title>
<meta http-equiv="content-type" content="text/html; charset=UTF-8">
<link rel="stylesheet" media="all" href="/attendant/css/docco.css">
</head>
<body>
<div id="container">
<div id="background"></div>
  <table cellspacing="0" cellpadding="0">
    <thead>
      <tr>
        <th class="docs"><h1>={ @file }</th>
        <th class="code"></th>
      </tr>
    </thead>
    <tbody>
      %{ for page in @page }:
        <tr>
          <td class="docs">-{ page.docco }</td>
          <td class="code"><div class="highlight"><pre>-{ page.source }</pre></div></td>
        </tr>
      %{ end }
    </tbody>
  </table>
</div>
</body>
</html>
